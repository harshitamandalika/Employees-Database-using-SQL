1. SELECT 
    dept_no
FROM
    departments;
    
2. SELECT 
    *
FROM
    departments;
    
3. SELECT 
    *
FROM
    employees
WHERE
    first_name = 'Elvis';
    
4. SELECT 
    *
FROM
    employees
WHERE
    gender = 'F' AND first_name = 'Kellie';
    
5. SELECT 
    *
FROM
    employees
WHERE
     first_name = 'Kellie' OR first_name='Aruna';
     
6. SELECT 
    *
FROM
    employees
WHERE
     gender = 'F' AND (first_name = 'Kellie' OR first_name='Aruna');
     
7. SELECT 
    *
FROM
    employees
WHERE
     first_name IN ('Denis', 'Elvis');
     
8. SELECT 
    *
FROM
    employees
WHERE
     first_name NOT IN ('John', 'Mark', 'Jacob');
     
9. SELECT 
    *
FROM
    employees
WHERE
     first_name LIKE 'Mark%';
     
10. SELECT 
    *
FROM
    employees
WHERE
     hire_date LIKE '%2000%';
     
11. SELECT 
    *
FROM
    employees
WHERE
     emp_no LIKE '1000_';
    
12. SELECT 
    *
FROM
    employees
WHERE
     first_name LIKE '%Jack%';
     
13. SELECT 
    *
FROM
    employees
WHERE
     first_name NOT LIKE '%Jack%';
     
14. SELECT 
    *
FROM
    salaries
WHERE
    salary BETWEEN 66000 AND 70000;
    
15. SELECT 
    *
FROM
    employees
WHERE
    emp_no NOT BETWEEN 10004 AND 10012;
    
16. SELECT 
    dept_name
FROM
    departments
WHERE
    dept_no BETWEEN 'd003' AND 'd006';
    
17. SELECT 
    dept_name
FROM
    departments
WHERE
    dept_no IS NOT NULL;
    
18. SELECT 
    *
FROM
    employees
WHERE
    gender = 'F' AND hire_date >= '2000-01-01';
    
19. SELECT 
    *
FROM
    salaries
WHERE
    salary >= 150000;
    
20. SELECT DISTINCT
    hire_date
FROM
    employees;
    
21. SELECT 
    COUNT(*)
FROM
    salaries
WHERE
    salary >= 10000;
    
22. SELECT 
    COUNT(*)
FROM
    dept_manager;
    
23. SELECT 
    *
FROM
    employees
ORDER BY hire_date DESC;

24. SELECT 
    salary, COUNT(salary) AS emps_with_same_salary
FROM
    salaries
WHERE
    salary > 80000
GROUP BY salary
ORDER BY salary;

25. SELECT 
    emp_no, avg(salary)
FROM
    salaries
GROUP BY emp_no
HAVING AVG(salary) > 120000;

26. SELECT 
    emp_no
FROM
    dept_emp
WHERE
    from_date > '2000-01-01'
GROUP BY emp_no
HAVING COUNT(from_date) > 1;

27. SELECT 
    *
FROM
    dept_emp
LIMIT 100;





     
